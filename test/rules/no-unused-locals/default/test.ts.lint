[typescript]: >= 2.9.1

// Error cases

// 6192
import { a, b } from "unused"
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ [All imports in import declaration are unused.]

// 6133
const c = 1
      ~ ['c' is declared but its value is never read.]

const obj = { d: 2, e: 3 }
// 6198
const { d, e } = obj
      ~~~~~~~~ [All destructured elements are unused.]

// 6199
var f, g
~~~~~~~~ [All variables are unused.]

// 6196
interface INotUsed {}
          ~~~~~~~~ ['INotUsed' is declared but never used.]

// Valid cases
const h = 1
console.log(h)

const obj2 = { i: 2, j: 3 }
const { i, j } = obj2
console.log(i, j)

export interface IUsed {}

var k, l
console.log(k, l)

// Allow unused parameters with this rule
export class Bar {
  constructor(private n: number) {
  }
}